# Required for Telegram API
API_ID=your_api_id_here
API_HASH=your_api_hash_here

# Required if no session exists or for initial login
PHONE_NUMBER=+621234567890

# Optional: 2FA password (if you use Two-Step Verification on Telegram)
TELEGRAM_PASSWORD=your_telegram_2fa_password

# Optional: Fallback Discord webhook URL if a channel is not mapped
DEFAULT_DISCORD_WEBHOOK=https://discord.com/api/webhooks/your_default_webhook_here
